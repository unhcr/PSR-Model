<userDefinedSQL>SELECT ITM_ID,&lt;br/&gt;  LANG_CODE,&lt;br/&gt;  TAB_ALIAS,&lt;br/&gt;  TEXT DESCRIPTION&lt;br/&gt;FROM&lt;br/&gt;  (SELECT TXT.ITM_ID,&lt;br/&gt;    TXT.LANG_CODE,&lt;br/&gt;    TXT.TAB_ALIAS,&lt;br/&gt;    TXT.TEXT,&lt;br/&gt;    row_number() OVER (PARTITION BY TXT.ITM_ID ORDER BY LANG.ACTIVE_FLAG DESC, NVL(ULP.PREF_SEQ, LANG.DISPLAY_SEQ + 1e5)) RANK&lt;br/&gt;  FROM T_TEXT_ITEMS TXT&lt;br/&gt;  INNER JOIN T_LANGUAGES LANG&lt;br/&gt;  ON LANG.CODE = TXT.LANG_CODE&lt;br/&gt;  LEFT OUTER JOIN T_USER_LANGUAGE_PREFERENCES ULP&lt;br/&gt;  ON ULP.LANG_CODE    = TXT.LANG_CODE&lt;br/&gt;  AND ULP.USERID      = sys_context(&apos;PSR&apos;, &apos;USERID&apos;)&lt;br/&gt;  WHERE TXT.TXTT_CODE = &apos;DESCR&apos;&lt;br/&gt;  AND TXT.SEQ_NBR     = 1&lt;br/&gt;  )&lt;br/&gt;WHERE RANK = 1</userDefinedSQL>
<layoutSQL>&lt;?xml version = &apos;1.0&apos; encoding = &apos;UTF-8&apos;?&gt;&lt;br&gt;&lt;ActiveQueryBuilderLayout&gt;&lt;SQL&gt;Select ITM_ID, LANG_CODE, TAB_ALIAS, TEXT DESCRIPTION From (Select TXT.ITM_ID, TXT.LANG_CODE, TXT.TAB_ALIAS, TXT.TEXT, row_number() Over (Partition By TXT.ITM_ID Order By LANG.ACTIVE_FLAG Desc, NVL(ULP.PREF_SEQ, LANG.DISPLAY_SEQ + 1e5)) RANK From T_TEXT_ITEMS TXT Inner Join T_LANGUAGES LANG On LANG.CODE = TXT.LANG_CODE Left Outer Join T_USER_LANGUAGE_PREFERENCES ULP On ULP.LANG_CODE = TXT.LANG_CODE And ULP.USERID = sys_context(&apos;PSR&apos;, &apos;USERID&apos;) Where TXT.TXTT_CODE = &apos;DESCR&apos; And TXT.SEQ_NBR = 1) Where RANK = 1&lt;/SQL&gt;&lt;SubQueries&gt;&lt;SubQuery SQL=&quot;Select ITM_ID, LANG_CODE, TAB_ALIAS, TEXT DESCRIPTION From (Select TXT.ITM_ID, TXT.LANG_CODE, TXT.TAB_ALIAS, TXT.TEXT, row_number() Over (Partition By TXT.ITM_ID Order By LANG.ACTIVE_FLAG Desc, NVL(ULP.PREF_SEQ, LANG.DISPLAY_SEQ + 1e5)) RANK From T_TEXT_ITEMS TXT Inner Join T_LANGUAGES LANG On LANG.CODE = TXT.LANG_CODE Left Outer Join T_USER_LANGUAGE_PREFERENCES ULP On ULP.LANG_CODE = TXT.LANG_CODE And ULP.USERID = sys_context(&apos;PSR&apos;, &apos;USERID&apos;) Where TXT.TXTT_CODE = &apos;DESCR&apos; And TXT.SEQ_NBR = 1) Where RANK = 1&quot;&gt;&lt;UnionSubQuery&gt;&lt;DataSource SQL=&quot;(Select TXT.ITM_ID, TXT.LANG_CODE, TXT.TAB_ALIAS, TXT.TEXT, row_number() Over (Partition By TXT.ITM_ID Order By LANG.ACTIVE_FLAG Desc, NVL(ULP.PREF_SEQ, LANG.DISPLAY_SEQ + 1e5)) RANK From T_TEXT_ITEMS TXT Inner Join T_LANGUAGES LANG On LANG.CODE = TXT.LANG_CODE Left Outer Join T_USER_LANGUAGE_PREFERENCES ULP On ULP.LANG_CODE = TXT.LANG_CODE And ULP.USERID = sys_context(&apos;PSR&apos;, &apos;USERID&apos;) Where TXT.TXTT_CODE = &apos;DESCR&apos; And TXT.SEQ_NBR = 1)&quot; X=&quot;50&quot; Y=&quot;20&quot; Height=&quot;135&quot; Width=&quot;140&quot;/&gt;&lt;/UnionSubQuery&gt;&lt;/SubQuery&gt;&lt;SubQuery SQL=&quot;Select TXT.ITM_ID, TXT.LANG_CODE, TXT.TAB_ALIAS, TXT.TEXT, row_number() Over (Partition By TXT.ITM_ID Order By LANG.ACTIVE_FLAG Desc, NVL(ULP.PREF_SEQ, LANG.DISPLAY_SEQ + 1e5)) RANK From T_TEXT_ITEMS TXT Inner Join T_LANGUAGES LANG On LANG.CODE = TXT.LANG_CODE Left Outer Join T_USER_LANGUAGE_PREFERENCES ULP On ULP.LANG_CODE = TXT.LANG_CODE And ULP.USERID = sys_context(&apos;PSR&apos;, &apos;USERID&apos;) Where TXT.TXTT_CODE = &apos;DESCR&apos; And TXT.SEQ_NBR = 1&quot;&gt;&lt;UnionSubQuery&gt;&lt;DataSource SQL=&quot;T_TEXT_ITEMS TXT&quot; X=&quot;50&quot; Y=&quot;20&quot; Height=&quot;207&quot; Width=&quot;140&quot;/&gt;&lt;DataSource SQL=&quot;T_LANGUAGES LANG&quot; X=&quot;240&quot; Y=&quot;20&quot; Height=&quot;135&quot; Width=&quot;140&quot;/&gt;&lt;DataSource SQL=&quot;T_USER_LANGUAGE_PREFERENCES ULP&quot; X=&quot;430&quot; Y=&quot;20&quot; Height=&quot;153&quot; Width=&quot;140&quot;/&gt;&lt;/UnionSubQuery&gt;&lt;/SubQuery&gt;&lt;/SubQueries&gt;&lt;/ActiveQueryBuilderLayout&gt;</layoutSQL>

<userDefinedSQL>SELECT ID,&lt;br/&gt;  DESCRIPTION,&lt;br/&gt;  LANG_CODE,&lt;br/&gt;  AGP_CODE,&lt;br/&gt;  AGE_FROM,&lt;br/&gt;  AGE_TO,&lt;br/&gt;  ITM_ID,&lt;br/&gt;  VERSION_NBR&lt;br/&gt;FROM&lt;br/&gt;  (SELECT AGR.ID,&lt;br/&gt;    TXT.TEXT DESCRIPTION,&lt;br/&gt;    TXT.LANG_CODE,&lt;br/&gt;    AGR.AGP_CODE,&lt;br/&gt;    AGR.AGE_FROM,&lt;br/&gt;    AGR.AGE_TO,&lt;br/&gt;    AGR.ITM_ID,&lt;br/&gt;    AGR.VERSION_NBR,&lt;br/&gt;    row_number() OVER (PARTITION BY AGR.ID ORDER BY LANG.ACTIVE_FLAG, NVL(ULP.PREF_SEQ, LANG.DISPLAY_SEQ + 1e5)) RANK&lt;br/&gt;  FROM T_AGE_RANGES AGR&lt;br/&gt;  INNER JOIN T_TEXT_ITEMS TXT&lt;br/&gt;  ON TXT.ITM_ID     = AGR.ITM_ID&lt;br/&gt;  AND TXT.TXTT_CODE = &apos;DESCR&apos;&lt;br/&gt;  AND TXT.SEQ_NBR   = 1&lt;br/&gt;  INNER JOIN T_LANGUAGES LANG&lt;br/&gt;  ON LANG.CODE = TXT.LANG_CODE&lt;br/&gt;  LEFT OUTER JOIN T_USER_LANGUAGE_PREFERENCES ULP&lt;br/&gt;  ON ULP.LANG_CODE = TXT.LANG_CODE&lt;br/&gt;  AND ULP.USERID   = sys_context(&apos;PSR&apos;, &apos;USERID&apos;)&lt;br/&gt;  )&lt;br/&gt;WHERE RANK = 1</userDefinedSQL>
